{
  "private": true,
  "scripts": {
    "start:dev": "bnr -s start-dev",
    "build": "bnr -s run-build",
    "analyze": "bnr -s run-analysis"
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --config=webpack.config.babel.js --mode=development",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "run-build": {
      "command": "webpack --config=webpack.config.babel.js --mode=production",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "run-analysis": {
      "command": "webpack --config=webpack.config.babel.js --mode=production",
      "env": {
        "NODE_PATH": "./src",
        "WEBPACK_ANALYZE": "true"
      }
    }
  },
  "devDependencies": {
    "@anansi/babel-preset": "0.6.1",
    "@anansi/eslint-config": "^0.4.3",
    "@anansi/webpack-config": "0.2.2",
    "@babel/core": "^7.2.2",
    "better-npm-run": "^0.1.1",
    "eslint": "^5.10.0",
    "flow-bin": "^0.89.0",
    "react-hot-loader": "^4.6.3",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "found": "^0.3.14",
    "react": "^16.7.0-alpha.2",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.7.0-alpha.2",
    "redbox-react": "^1.6.0"
  }
}
